<facelet-taglib xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
    version="2.2">

	<namespace>http://www.metachart.org/ui</namespace>
  
	<tag>
        <tag-name>apexHeatBar</tag-name>
        <component>
            <resource-id>metachartApex/heatbar.xhtml</resource-id>
        </component>
	</tag>
    
    <tag>
        <tag-name>echartHello</tag-name>
        <component>
             <component-type>org.metachart.jsf.chart.echart.ChartHello</component-type>
        </component>
	</tag>
	
	<tag>
        <tag-name>echart</tag-name>
        <component>
             <component-type>org.metachart.jsf.chart.echart.Chart</component-type>
        </component>
	</tag>
	<tag>
        <tag-name>title</tag-name>
        <component>
             <component-type>org.metachart.jsf.common.Title</component-type>
        </component>
        <attribute>
            <name>text</name>
            <type>java.lang.String</type>
        </attribute>
	</tag>
	<tag>
        <tag-name>data</tag-name>
        <component>
             <component-type>org.metachart.jsf.common.Data</component-type>
        </component>
        <attribute>
            <name>value</name>
            <type>org.metachart.model.json.chart.echart.data.JsonData</type>
        </attribute>
	</tag>
	<tag>
        <tag-name>axis</tag-name>
        <component>
             <component-type>org.metachart.jsf.common.Axis</component-type>
        </component>
	</tag>
    
</facelet-taglib>